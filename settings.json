{
    "workbench.colorTheme": "focus",
    "workbench.iconTheme": "jetbrains-file-icon-theme-auto",
    "background.windowBackgrounds": [
    ],

    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.intelliSenseUpdateDelay": 500,
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.autocomplete": "default",
    "C_Cpp.loggingLevel": "Warning",
    "files.autoSave": "afterDelay",

    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": {
        "cpp": "g++ \"$fullFileName\" -o \"$fileNameWithoutExt\".exe && ./\"$fileNameWithoutExt\".exe",
        "c": "gcc \"$fullFileName\" -o \"$fileNameWithoutExt\" && ./\"$fileNameWithoutExt\"",
        "python": "python3 -u \"$fullFileName\""
    },

    "breadcrumbs.enabled": true,
    "editor.stickyScroll.enabled": false,
    "synthwave84.enableGlowingEffect": true,
    "CPcodeforces.userHandle": "Ekber_Ekber",
    "window.zoomLevel": 0,

    "auto-correct.dictionary": [
        {
        "languages": ["*"],
        "words": {
            "itn": "int",
            "contienu": "continue",
            "İNF": "INF",
            "logn": "long",
            "şş": ">>",
            "&I": "&i",
            "(Int": "(int"
        },
        "useLargeList": false
        }
    ],

    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#40FFE7",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "debug",
            "color": "#009478",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
    "editor.fontSize": 17,
    "editor.cursorBlinking": "smooth",
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "editor.fontLigatures": true,
    "editor.fontFamily": "JetBrainsMono Nerd Font, monospace",
    "terminal.integrated.fontFamily": "monospace",
    "background.useInvertedOpacity": true,
    "cph.general.defaultLanguage": "cpp",

}
