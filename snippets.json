            "\tvoid build(int v, int tl, int tr) {",
            "\t\tif (tl == tr) {",
            "\t\t\tt[v] = a[tl];",
            "\t\t\treturn;",
            "\t\t}",
            "\t\tint tm = (tl + tr) / 2;",
            "\t\tbuild(2*v, tl, tm);",
            "\t\tbuild(2*v+1, tm+1, tr);",
            "\t\tt[v] = merge(t[v*2], t[v*2+1]);",
            "\t}",
            "\tint find(int v, int tl, int tr, int l, int r) {",
            "\t\tif (l > r) return INF;      // TODO",
